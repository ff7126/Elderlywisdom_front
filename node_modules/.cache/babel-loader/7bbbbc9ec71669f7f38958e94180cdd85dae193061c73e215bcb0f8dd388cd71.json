age(\"删除成功\")\r\n      }).catch(error => {\r\n        console.log(error)\r\n      })\r\n    },\r\n    //添加老人\r\n    add() {\r\n      this.oldMesTitle='新增老人信息'\r\n      this.oldMesType=3\r\n      this.oldMesVisible=true\r\n    },\r\n    //查看老人\r\n    viewMan(item){\r\n      this.oldMesTitle='查看老人信息'\r\n      this.oldMesType=1\r\n      this.oldMesVisible=true\r\n      elderByID(item.id).then(res => {\r\n        console.log(this.oldMesVisible,1553)\r\n        this.form=res.data\r\n        // this.elderList= res.data.records\r\n        // this.listForm.total=res.data.records.length\r\n      }).catch(error => {\r\n        console.log(error)\r\n      })\r\n      // this.$router.push('/oldMes')\r\n    },\r\n    updateMan(item){\r\n      this.oldMesTitle='修改老人信息'\r\n      this.oldMesType=2\r\n      this.oldMesVisible=true\r\n      elderByID(item.id).then(res => {\r\n        console.log(this.oldMesVisible,1553)\r\n        this.form=res.data\r\n        // this.elderList= res.data.records\r\n        // this.listForm.total=res.data.records.length\r\n      }).catch(error => {\r\n        console.log(error)\r\n      })\r\n    },\r\n    updateOld(){\r\n\r\n    },\r\n    handleSizeChange(value){\r\n      this.listForm.size = value;\r\n      this.getElderList()\r\n    },\r\n    handlePageChange(value){\r\n      this.listForm.page = value;\r\n      this.getElderList()\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\ni#icon{\r\n  font-size:xx-large;\r\n  float:right;\r\n  margin-top: 10px;\r\n  margin-right:5px;\r\n}\r\n</style>\r\n"],"mappings":"AA2HA,OAAAA,MAAA;AACA,SAAAC,WAAA,EAAAC,SAAA,EAAAC,SAAA;AAEA;EACAC,UAAA;IAAAJ;EAAA;EACAK,KAAA;IACA;MACAC,QAAA;QACAC,IAAA;QACAC,IAAA;QACAC,KAAA;QACAC,UAAA;MAEA;MACAP,SAAA;MACAQ,aAAA;MACAC,WAAA;MACAC,UAAA;MAAA;MACAC,IAAA;QACAC,cAAA;QACAC,aAAA;QACAC,SAAA;QACAC,EAAA;QACAC,QAAA;QACAC,MAAA;QACAC,KAAA;QACAC,EAAA;QACAC,QAAA;QACAC,OAAA;QACAC,iBAAA;QACAC,kBAAA;QACAC,mBAAA;QACAC,cAAA;QACAC,eAAA;QACAC,gBAAA;QACAC,YAAA;QACAC,GAAA;QACAC,UAAA;QACAC