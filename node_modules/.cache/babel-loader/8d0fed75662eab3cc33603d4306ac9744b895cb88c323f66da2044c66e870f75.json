{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"LoginComponent\",\n  data() {\n    return {\n      active: {\n        login: true,\n        register: false\n      },\n      showPassword: false,\n      loginForm: {\n        username: '',\n        password: ''\n      },\n      registerForm: {\n        username: '',\n        realname: '',\n        gender: '',\n        phone: '',\n        password: '',\n        email: ''\n      }\n    };\n  },\n  methods: {\n    togglePasswordVisibility() {\n      this.showPassword = !this.showPassword;\n    },\n    go(type) {\n      this.active.login = type === 'login';\n      this.active.register = type === 'register';\n    },\n    submit() {\n      if (this.active.login) {\n        fetch('http://xiao.free.idcfengye.com/api/admin/login', {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify({\n            username: this.loginForm.username,\n            password: this.loginForm.password\n          })\n        }).then(response => response.json()).then(data => {\n          if (data.code === 0) {\n            this.$router.push('/oldMen');\n          } else {\n            console.log(data);\n            this.$message.error('登录失败，请检查用户名或密码');\n          }\n        }).catch(error => {\n          console.error(error);\n          this.$message.error('登录失败，请稍后再试');\n        });\n      } else {\n        fetch('http://xiao.free.idcfengye.com/api/admin/register', {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify({\n            username: this.registerForm.username,\n            password: this.registerForm.password,\n            real_name: this.registerForm.realname,\n            gender: this.registerForm.gender,\n            email: this.registerForm.email,\n            phone: this.registerForm.phone\n          })\n      