     console.log(error);\n      });\n    },\n    updateMan(item) {\n      this.oldMesTitle = '修改老人信息';\n      this.oldMesType = 2;\n      this.oldMesVisible = true;\n      axios.get(`http://xiao.free.idcfengye.com/api/oldperson/search`, {\n        params: {\n          id: item.id\n        }\n      }).then(res => {\n        this.form = res.data.data;\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    updateOld() {\n      axios.put(`http://xiao.free.idcfengye.com/api/oldperson/${this.form.id}`, this.form).then(res => {\n        this.getElderList();\n      }).catch(error => {\n        console.log(error);\n      });\n      this.oldMesVisible = false;\n    },\n    addOld() {\n      axios.post(\"http://xiao.free.idcfengye.com/api/oldperson\", this.form).then(res => {\n        this.getElderList();\n      }).catch(error => {\n        console.log(error);\n      });\n      this.oldMesVisible = false;\n    },\n    handleSizeChange(value) {\n      this.listForm.size = value;\n      this.getElderList();\n    },\n    handlePageChange(value) {\n      this.listForm.page = value;\n      this.getElderList();\n    }\n  }\n};","map":{"version":3,"names":["axios","MessageBox","addOldSchedule","addSchedule","data","listForm","page","size","total","pagerCount","genderOptions","value","label","elderList","gender","oldMesVisible","oldMesTitle","selectedOld","oldMesType","dialogFormVisible","form","dialogImageUrl","dialogVisible","fileParam","id","username","phone","id_card","birthday","address","firstGuardianName","firstGuardianPhone","firstGuardianWechat","guardian2_name","guardian2_phone","guardian2_wechat","health_state","des","createTime","createName","updateTime","updateName","calendarForm","date","type","title","mounted","getElderList","methods","calculateAge","birthDate","Date","today","age","getFullYear","monthDifference","getMonth","getDate","old_face","item","localStorage","setItem","$router","push","get","params","then","res","JSON","stringify","addDa