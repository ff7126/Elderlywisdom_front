{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport { MessageBox } from 'element-ui';\nimport { addOldSchedule, addSchedule } from \"@/api/schedule\";\nexport default {\n  data() {\n    return {\n      listForm: {\n        page: 1,\n        size: 20,\n        total: 0,\n        pagerCount: 5\n      },\n      genderOptions: [{\n        value: '0',\n        label: '女'\n      }, {\n        value: '1',\n        label: '男'\n      }],\n      elderList: [],\n      gender: ['女', '男'],\n      oldMesVisible: false,\n      oldMesTitle: '查看老人信息',\n      selectedOld: 0,\n      oldMesType: 1,\n      // 1仅查看2修改3新增\n      dialogFormVisible: false,\n      form: {\n        dialogImageUrl: \"\",\n        dialogVisible: false,\n        fileParam: \"\",\n        id: '',\n        username: '',\n        gender: '',\n        phone: '',\n        id_card: '',\n        birthday: '',\n        address: '',\n        checkin_date: '',\n        firstguardian_relationship: '',\n        secondguardian_relationship: '',\n        room_number: '',\n        firstguardian_name: '',\n        firstguardian_phone: '',\n        firstguardian_wechat: '',\n        secondguardian_name: '',\n        secondguardian_phone: '',\n        secondguardian_wechat: '',\n        health_state: '',\n        des: ''\n      },\n      calendarForm: {\n        date: '',\n        type: '',\n        title: ''\n      }\n    };\n  },\n  mounted() {\n    this.getElderList();\n  },\n  methods: {\n    async captureFace(item) {\n      try {\n        // 请求用户允许使用摄像头\n        const stream = await navigator.mediaDevices.getUserMedia({\n          video: true\n        });\n        // 显示视频流到页面上，用于实时显示\n        const video = document.createElement('video');\n        video.srcObject = stream;\n        video.play();\n\n        // 弹窗或其他方式展示视频流\n        this.showVideoDialog(video);\n\n        // 录制视频\n        let recorder = new MediaRecorder(stream);\n        let data = [];\n        recorder.ondataavailable = event => data.push(event.data);\n        recorder.start();\n\n        // 5秒后停止录制\n        setTimeout(async () => {\n          recorder.stop();\n\n          // 停止摄像头\n          stream.getTracks().forEach(track => track.stop());\n          video.srcObject = null;\n\n          // 处理并发送数据到后端\n          let videoBlob = new Blob(data, {\n            type: 'video/webm'\n          });\n          let formData = new FormData();\n          formData.append('video', videoBlob, 'face.webm');\n\n          // 发送请求到服务器\n          await axios.post('你的后端API地址', formData, {\n            headers: {\n              'Content-Type': 'multipart/form-data'\n            }\n          });\n          this.$message.success('上传成功');\n        }, 5000);\n      } catch (error) {\n        console.error('捕获视频时出错:', error);\n        this.$message.error('无法打开摄像头');\n      }\n    },\n    showVideoDialog(videoElement) {\n      // 这里可以创建一个弹窗来展示videoElement\n      // 或者将videoElement添加到现有的DOM中\n      const dialog = document.createElement('div');\n      dialog.appendChild(videoElement);\n      document.body.appendChild(dialog);\n    },\n    calculateAge(birthday) {\n      const birthDate = new Date(birthday);\n      const today = new Date();\n      let age = today.getFullYear() - birthDate.getFullYear();\n      const monthDifference = today.getMonth() - birthDate.getMonth();\n      if (monthDifference < 0 || monthDifference === 0 && today.getDate() < birthDate.getDate()) {\n        age--;\n      }\n      return age;\n    },\n    old_face(item) {\n      var id = item.id;\n      localStorage.setItem(\"old_id\", id);\n      this.$router.push('/old_face');\n      axios.get(\"http://xiao.free.idcfengye.com/api/oldperson\", {\n        params: {\n          id: id\n        }\n      }).then(res => {\n        localStorage.setItem(\"old_face\", JSON.stringify(res));\n      });\n    },\n    addDate(item) {\n      addOldSchedule(item.id).then(res => {\n        this.dialogFormVisible = true;\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    addCalendar() {\n      addSchedule(this.calendarForm).then(res => {\n        this.$message.success(\"添加成功\");\n      }).catch(error => {\n        console.log(error);\n      });\n      this.dialogFormVisible = false;\n    },\n    getElderList() {\n      axios.get(\"http://xiao.free.idcfengye.com/api/oldperson\").then(res => {\n        if (res.data.code === 0) {\n          this.elderList = res.data.data;\n        } else {\n          this.$message.error('获取老人信息失败');\n        }\n      }).catch(error => {\n        console.error(error);\n        this.$message.error('获取老人信息失败，请稍后再试');\n      });\n    },\n    delElder(item) {\n      axios.delete(`http://xiao.free.idcfengye.com/api/oldperson/${item.id}`).then(res => {\n        this.$message(\"删除成功\");\n        this.getElderList();\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    confirmDelete(item) {\n      MessageBox.confirm('确定要删除吗？', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        this.delElder(item);\n      }).catch(() => {});\n    },\n    add() {\n      this.oldMesTitle = '新增老人信息';\n      this.oldMesType = 3;\n      this.oldMesVisible = true;\n    },\n    viewMan(item) {\n      this.oldMesTitle = '查看老人信息';\n      this.oldMesType = 1;\n      axios.get(`http://xiao.free.idcfengye.com/api/oldperson/search`, {\n        params: {\n          id: item.id\n        }\n      }).then(res => {\n        this.form = res.data.data[0];\n        this.oldMesVisible = true;\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    updateMan(item) {\n      this.oldMesTitle = '修改老人信息';\n      this.oldMesType = 2;\n      axios.get(`http://xiao.free.idcfengye.com/api/oldperson/search`, {\n        params: {\n          id: item.id\n        }\n      }).then(res => {\n        this.form = res.data.data[0];\n        this.oldMesVisible = true;\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    updateOld() {\n      axios.put(`http://xiao.free.idcfengye.com/api/oldperson/${this.form.id}`, this.form).then(res => {\n        this.getElderList();\n      }).catch(error => {\n        console.log(error);\n      });\n      this.oldMesVisible = false;\n    },\n    addOld() {\n      axios.post(\"http://xiao.free.idcfengye.com/api/oldperson\", this.form).then(res => {\n        this.getElderList();\n      }).catch(error => {\n        console.log(error);\n      });\n      this.oldMesVisible = false;\n    },\n    handleSizeChange(value) {\n      this.listForm.size = value;\n      this.getElderList();\n    },\n    handlePageChange(value) {\n      this.listForm.page = value;\n      this.getElderList();\n    }\n  }\n};","map":{"version":3,"names":["axios","MessageBox","addOldSchedule","addSchedule","data","listForm","page","size","total","pagerCount","genderOptions","value","label","elderList","gender","oldMesVisible","oldMesTitle","selectedOld","oldMesType","dialogFormVisible","form","dialogImageUrl","dialogVisible","fileParam","id","username","phone","id_card","birthday","address","checkin_date","firstguardian_relationship","secondguardian_relationship","room_number","firstguardian_name","firstguardian_phone","firstguardian_wechat","secondguardian_name","secondguardian_phone","secondguardian_wechat","health_state","des","calendarForm","date","type","title","mounted","getElderList","methods","captureFace","item","stream","navigator","mediaDevices","getUserMedia","video","document","createElement","srcObject","play","showVideoDialog","recorder","MediaRecorder","ondataavailable","event","push","start","setTimeout","stop","getTracks","forEach","track","videoBlob","Blob","formData","FormData","append","post","headers","$message","success","error","console","videoElement","dialog","appendChild","body","calculateAge","birthDate","Date","today","age","getFullYear","monthDifference","getMonth","getDate","old_face","