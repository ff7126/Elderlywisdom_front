{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      tableData: [],\n      selectedEventType: '',\n      pagination: {\n        currentPage: 1,\n        pageSize: 10,\n        totalItems: 0\n      }\n    };\n  },\n  mounted() {\n    this.getData();\n  },\n  methods: {\n    getData() {\n      const eventType = encodeURIComponent(this.selectedEventType);\n      const url = this.selectedEventType ? `http://192.168.57.26:5001/api/statistic/events?event_type=${eventType}` : `http://192.168.57.26:5001/api/statistic/events`;\n      this.$axios.get(url).then(res => {\n        if (res.data.code === 0) {\n          this.tableData = res.data.data;\n          this.pagination.totalItems = this.tableData.length;\n        } else {\n          this.$message.error(res.data.message || 'Failed to fetch data');\n        }\n      }).catch(error => {\n        console.error(\"Error fetching data: \", error);\n        this.$message.error(\"服务器错误，请稍后再试！\");\n      });\n    },\n    handleSizeChange(newSize) {\n      this.pagination.pageSize = newSize;\n      this.pagination.currentPage = 1;\n      this.getData();\n    },\n    handleCurrentChange(newPage) {\n      this.pagination.currentPage = newPage;\n      this.getData();\n    }\n  }\n};","map":{"version":3,"names":["axios","data","tableData","selectedEventType","pagination","currentPage","pageSize","totalItems","mounted","getData","methods","eventType","encodeURIComponent","url","$axios","get","then","res","code","length","$message","error","message","catch","console","handleSizeChange","newSize","handleCurrentChange","newPage"],"sources":["src/views/page/record/abnormal.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <el-select v-model=\"selectedEventType\" placeholder=\"选择事件类型\" @change=\"getData\">\r\n      <el-option label=\"Smoking\" value=\"Smoking\"></el-option>\r\n      <el-option label=\"Tired Sleeping\" value=\"TiredSleeping\"></el-option>\r\n      <el-option label=\"Using Phone\" value=\"UsingPhone\"></el-option>\r\n      <el-option label=\"Drinking\" value=\"Drinking\"></el-option>\r\n    </el-select>\r\n    <el-table :data=\"tableData.slice((pagination.currentPage - 1) * pagination.pageSize, pagination.currentPage * pagination.pageSize)\" border style=\"width: 100%\">\r\n      <el-table-column prop=\"event_picture\" label=\"截图\" width=\"180\">\r\n        <template slot-scope=\"scope\">\r\n          <img :src=\"scope.row.event_picture\" alt=\"Event Image\" style=\"width: 100px; height: 100px;\">\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"event_time\" label=\"时间\" width=\"180\"></el-table-column>\r\n      <el-table-column prop=\"event_type\" label=\"事件类型\" width=\"190\"></el-table-column>\r\n      <el-table-column label=\"视频\" width=\"190\">\r\n        <template slot-scope=\"scope\">\r\n          <video controls style=\"width: 180px; height: 100px;\">\r\n            <source :src=\"scope.row.event_video\" type=\"video/mp4\">\r\n          </video>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"pagination.currentPage\"\r\n        :page-sizes=\"[10, 20, 50, 100]\"\r\n        :page-size=\"pagination.pageSize\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"pagination.totalItems\"\r\n        style=\"margin-top: 20px;\">\r\n    </el-pagination>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      selectedEventType: '',\r\n      pagination: {\r\n        currentPage: 1,\r\n        pageSize: 10,\r\n        totalItems: 0\r\n      }\r\n    }\r\n  },\r\n  mounted() {\r\n    this.getData();\r\n  },\r\n  methods: {\r\n    getData() {\r\n      const eventType = encodeURIComponent(this.selectedEventType);\r\n      const url = this.selectedEventType\r\n          ? `http://192.168.57.26:5001/api/statistic/events?event_type=${eventType}`\r\n          : `http://192.168.57.26:5001/api/statistic/events`;\r\n\r\n      this.$axios.get(url)\r\n          .then(res => {\r\n            if (res.data.code === 0) {\r\n              this.tableData = res.data.data;\r\n              this.pagination.totalItems = this.tableData.length;\r\n            } else {\r\n              this.$message.error(res.data.message || 'Failed to fetch data');\r\n            }\r\n          })\r\n          .catch(error => {\r\n            console.error(\"Error fetching data: \", error);\r\n            this.$message.error(\"服务器错误，请稍后再试！\");\r\n          });\r\n    },\r\n    handleSizeChange(newSize) {\r\n      this.pagination.pageSize = newSize;\r\n      this.pagination.currentPage = 1;\r\n      this.getData();\r\n    },\r\n    handleCurrentChange(newPage) {\r\n      this.pagination.currentPage = newPage;\r\n      this.getData();\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n"],"mappings":"AAsCA,OAAAA,KAAA;AAEA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,iBAAA;MACAC,UAAA;QACAC,WAAA;QACAC,QAAA;QACAC,UAAA;MACA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,OAAA;EACA;EACAC,OAAA;IACAD,QAAA;MACA,MAAAE,SAAA,GAAAC,kBAAA,MAAAT,iBAAA;MACA,MAAAU,GAAA,QAAAV,iBAAA,GACA,6DAAAQ,SAAA,KACA;MAEA,KAAAG,MAAA,CAAAC,GAAA,CAAAF,GAAA,EACAG,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAhB,IAAA,CAAAiB,IAAA;UACA,KAAAhB,SAAA,GAAAe,GAAA,CAAAhB,IAAA,CAAAA,IAAA;UACA,KAAAG,UAAA,CAAAG,UAAA,QAAAL,SAAA,CAAAiB,MAAA;QACA;UACA,KAAAC,QAAA,CAAAC,KAAA,CAAAJ,GAAA,CAAAhB,IAAA,CAAAqB,OAAA;QACA;MACA,GACAC,KAAA,CAAAF,KAAA;QACAG,OAAA,CAAAH,KAAA,0BAAAA,KAAA;QACA,KAAAD,QAAA,CAAAC,KAAA;MACA;IACA;IACAI,iBAAAC,OAAA;MACA,KAAAtB,UAAA,CAAAE,QAAA,GAAAoB,OAAA;MACA,KAAAtB,UAAA,CAAAC,WAAA;MACA,KAAAI,OAAA;IACA;IACAkB,oBAAAC,OAAA;MACA,KAAAxB,UAAA,CAAAC,WAAA,GAAAuB,OAAA;MACA,KAAAnB,OAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}