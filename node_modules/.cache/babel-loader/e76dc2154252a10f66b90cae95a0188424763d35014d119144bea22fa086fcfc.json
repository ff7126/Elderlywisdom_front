{"ast":null,"code":"import { ref } from 'vue';\nexport default {\n  name: \"LoginComponent\",\n  setup() {\n    const active = ref({\n      login: true,\n      register: false\n    });\n    function go(type) {\n      active.value = {\n        login: type === 'login',\n        register: type === 'register'\n      };\n    }\n    function submit() {\n      // Implement submission logic here\n    }\n    return {\n      active,\n      go,\n      submit\n    };\n  }\n};","map":{"version":3,"names":["ref","name","setup","active","login","register","go","type","value","submit"],"sources":["src/views/login/Login.vue"],"sourcesContent":["<template>\r\n  <div class=\"content_login\">\r\n    <div id=\"app\" class=\"container\">\r\n      <img src=\"@/assets/111.png\" alt=\"\">\r\n      <div class=\"panel\">\r\n        <div class=\"switch\">\r\n          <span :class='{\"active\": active.login}' @click=\"go('login')\">登录</span>\r\n          <span>/</span>\r\n          <span v-if=\"!active.register\" @click=\"go('register')\">还没有账号？点击注册</span>\r\n        </div>\r\n        <div class=\"form\" id=\"fromLogin\">\r\n          <el-form ref=\"registerForm\" v-if=\"active.register\">\r\n            <!-- 注册表单 -->\r\n            <el-form-item prop=\"username\" class=\"input\">\r\n              <el-input placeholder=\"请输入用户名\" size=\"medium\" style=\"margin: 10px 0\" prefix-icon=\"el-icon-user\" v-model=\"registerForm.username\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item prop=\"phone\" class=\"input\">\r\n              <el-input placeholder=\"请输入手机号码\" size=\"medium\" style=\"margin: 10px 0\" prefix-icon=\"el-icon-phone\" v-model=\"registerForm.phone\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item prop=\"password\" class=\"input\">\r\n              <el-input placeholder=\"请输入密码\" size=\"medium\" style=\"margin: 10px 0\" prefix-icon=\"el-icon-aim\" show-password v-model=\"registerForm.password\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item prop=\"realName\" class=\"input\">\r\n              <el-input placeholder=\"请输入真实姓名\" size=\"medium\" style=\"margin: 10px 0\" prefix-icon=\"el-icon-user\" v-model=\"registerForm.realName\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item prop=\"email\" class=\"input\">\r\n              <el-input placeholder=\"请输入邮箱\" size=\"medium\" style=\"margin: 10px 0\" prefix-icon=\"el-icon-mail\" v-model=\"registerForm.email\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item>\r\n              <el-button type=\"primary\" @click=\"submit\">注册</el-button>\r\n            </el-form-item>\r\n          </el-form>\r\n\r\n          <el-form ref=\"loginForm\" v-if=\"active.login\">\r\n            <!-- 登录表单 -->\r\n            <el-form-item prop=\"username\" class=\"input\">\r\n              <el-input placeholder=\"请输入手机号\" size=\"medium\" style=\"margin: 15px 0\" prefix-icon=\"el-icon-user\" v-model=\"loginForm.phone\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item prop=\"password\" class=\"input\">\r\n              <el-input placeholder=\"请输入密码\" size=\"medium\" style=\"margin: 10px 0\" prefix-icon=\"el-icon-aim\" show-password v-model=\"loginForm.password\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item>\r\n              <el-button type=\"primary\" @click=\"submit\">登录</el-button>\r\n            </el-form-item>\r\n          </el-form>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref } from 'vue';\r\n\r\nexport default {\r\n  name: \"LoginComponent\",\r\n  setup() {\r\n    const active = ref({ login: true, register: false });\r\n\r\n    function go(type) {\r\n      active.value = {\r\n        login: type === 'login',\r\n        register: type === 'register'\r\n      };\r\n    }\r\n\r\n    function submit() {\r\n      // Implement submission logic here\r\n    }\r\n\r\n    return { active, go, submit };\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.content_login {\r\n  height: 100vh;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  background: linear-gradient(45deg, rgb(250, 229, 247), rgb(246, 195, 229)) fixed;\r\n}\r\n\r\n.container {\r\n  position: relative;\r\n  width: 70rem;\r\n}\r\n\r\n.container img {\r\n  width: 70rem;\r\n}\r\n\r\n.panel {\r\n  width: 44%;\r\n  margin: 4rem 0 0;\r\n  position: absolute;\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n\r\n.switch span {\r\n  color: #ccc;\r\n  font-size: 1.4rem;\r\n  cursor: pointer;\r\n}\r\n\r\n.switch span.active {\r\n  color: rgb(222, 18, 181);\r\n}\r\n\r\n.form {\r\n  width: 30rem;\r\n  margin-top: 3rem;\r\n}\r\n\r\n.form .input {\r\n  margin: 1rem 0;\r\n}\r\n\r\n.form el-input {\r\n  border-bottom: .1rem solid rgb(194, 52, 191);\r\n  line-height: 35px;\r\n  background-color: transparent;\r\n}\r\n\r\n.form el-button {\r\n  width: 100%;\r\n  height: 3rem;\r\n  border-radius: 3rem;\r\n  background: linear-gradient(45deg, rgb(166, 111, 182), rgb(248, 127, 241));\r\n  box-shadow: 0 0 8px rgb(177, 98, 183);\r\n  color: white;\r\n}\r\n</style>\r\n"],"mappings":"AAoDA,SAAAA,GAAA;AAEA;EACAC,IAAA;EACAC,MAAA;IACA,MAAAC,MAAA,GAAAH,GAAA;MAAAI,KAAA;MAAAC,QAAA;IAAA;IAEA,SAAAC,GAAAC,IAAA;MACAJ,MAAA,CAAAK,KAAA;QACAJ,KAAA,EAAAG,IAAA;QACAF,QAAA,EAAAE,IAAA;MACA;IACA;IAEA,SAAAE,OAAA;MACA;IAAA;IAGA;MAAAN,MAAA;MAAAG,EAAA;MAAAG;IAAA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}