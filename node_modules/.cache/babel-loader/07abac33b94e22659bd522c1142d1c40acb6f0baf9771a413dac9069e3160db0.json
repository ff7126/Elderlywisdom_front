></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"密码：\" prop=\"password\" class=\"input\">\r\n              <el-input placeholder=\"请输入密码\" size=\"medium\" prefix-icon=\"el-icon-aim\" show-password v-model=\"loginForm.password\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item>\r\n              <el-button type=\"primary\" @click=\"submit('loginForm')\" class=\"loginbutton\">登录</el-button>\r\n            </el-form-item>\r\n            <div class=\"register-prompt\">\r\n              <div>还没有账号？</div>\r\n              <div class=\"register-link\" @click=\"go('register')\">点击注册</div>\r\n            </div>\r\n          </el-form>\r\n        </div>\r\n\r\n        <!-- 注册表单 -->\r\n        <div class=\"form\" v-if=\"active.register\">\r\n          <el-form ref=\"registerForm\" :model=\"registerForm\" :rules=\"registerRules\" label-width=\"100px\">\r\n            <el-form-item label=\"真实姓名：\" prop=\"real_name\" class=\"input\">\r\n              <el-input placeholder=\"请输入真实姓名\" size=\"medium\" prefix-icon=\"el-icon-user\" v-model=\"registerForm.real_name\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"用户名：\" prop=\"username1\" class=\"input\">\r\n              <el-input placeholder=\"请输入用户名\" size=\"medium\" prefix-icon=\"el-icon-user\" v-model=\"registerForm.username1\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"密码：\" prop=\"password1\" class=\"input\">\r\n              <el-input placeholder=\"请输入密码\" size=\"medium\" prefix-icon=\"el-icon-aim\" show-password v-model=\"registerForm.password1\" aria-required=\"true\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"性别：\" prop=\"gender\" class=\"input\">\r\n              <el-select v-model=\"registerForm.gender\" placeholder=\"请选择性别\" size=\"medium\">\r\n                <el-option label=\"男\" value=\"1\"></el-option>\r\n                <el-option label=\"女\" value=\"0\"></