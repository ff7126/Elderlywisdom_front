{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport { MessageBox } from 'element-ui';\nimport { addOldSchedule, addSchedule } from \"@/api/schedule\";\nexport default {\n  data() {\n    return {\n      dialogVisible: false,\n      // 控制对话框显示的变量\n      listForm: {\n        page: 1,\n        size: 20,\n        total: 0,\n        pagerCount: 5\n      },\n      genderOptions: [{\n        value: '0',\n        label: '女'\n      }, {\n        value: '1',\n        label: '男'\n      }],\n      elderList: [],\n      gender: ['女', '男'],\n      oldMesVisible: false,\n      oldMesTitle: '查看老人信息',\n      selectedOld: 0,\n      oldMesType: 1,\n      // 1仅查看2修改3新增\n      dialogFormVisible: false,\n      form: {\n        dialogImageUrl: \"\",\n        dialogVisible: false,\n        fileParam: \"\",\n        id: '',\n        username: '',\n        gender: '',\n        phone: '',\n        id_card: '',\n        birthday: '',\n        address: '',\n        checkin_date: '',\n        firstguardian_relationship: '',\n        secondguardian_relationship: '',\n        room_number: '',\n        firstguardian_name: '',\n        firstguardian_phone: '',\n        firstguardian_wechat: '',\n        secondguardian_name: '',\n        secondguardian_phone: '',\n        secondguardian_wechat: '',\n        health_state: '',\n        des: ''\n      },\n      calendarForm: {\n        date: '',\n        type: '',\n        title: ''\n      }\n    };\n  },\n  mounted() {\n    this.getElderList();\n  },\n  methods: {\n    captureFace(row) {\n      this.dialogVisible = true;\n\n      // Request access to user's camera\n      navigator.mediaDevices.getUserMedia({\n        video: true\n      }).then(stream => {\n        const videoElement = document.getElementById('videoElement');\n        videoElement.srcObject = stream;\n        const chunks = [];\n        const recorder = new MediaRecorder(stream);\n        recorder.ondataavailable = event => {\n          chunks.push(event.data);\n        };\n        recorder.start(5000); // Record for 5 seconds\n\n        setTimeout(() => {\n          recorder.stop();\n          stream.getVideoTracks()[0].stop(); // Stop camera stream\n        }, 5000);\n        recorder.onstop = () => {\n          // Create FormData and append recorded video data\n          const formData = new FormData();\n          formData.append('file', new Blob(chunks, {\n            type: 'video/mp4'\n          }));\n          formData.append('username', row.username); // Pass username\n          formData.append('id', row.id); // Pass ID\n          console.log(\"Uploaded file type:\", file.type);\n\n          // Example: Send FormData to backend using Axios\n          axios.post('http://fan.v1.idcfengye.com/upload_video', formData).then(response => {\n            console.log('Video upload successful:', response.data);\n            // Optional: Handle response or update UI\n          }).catch(error => {\n            console.error('Error uploading video:', error);\n            // Handle errors or display error message\n          }).finally(() => {\n            this.dialogVisible = false; // Close dialog\n          });\n        };\n      }).catch(error => {\n        console.error('Error accessing camera:', error);\n        // Handle camera access errors\n        this.dialogVisible = false; // Close dialog on error\n      });\n    },\n\n    handleClose(done) {\n      done();\n    },\n    calculateAge(birthday) {\n      const birthDate = new Date(birthday);\n      const today = new Date();\n      let age = today.getFullYear() - birthDate.getFullYear();\n      const monthDifference = today.getMonth() - birthDate.getMonth();\n      if (monthDifference < 0 || monthDifference === 0 && today.getDate() < birthDate.getDate()) {\n        age--;\n      }\n      return age;\n    },\n    old_face(item) {\n      var id = item.id;\n      localStorage.setItem(\"old_id\", id);\n      this.$router.push('/old_face');\n      axios.get(\"http://xiao.free.idcfengye.com/api/oldperson\", {\n        params: {\n          id: id\n        }\n      }).then(res => {\n        localStorage.setItem(\"old_face\", JSON.stringify(res));\n      });\n    },\n    addDate(item) {\n      addOldSchedule(item.id).then(res => {\n        this.dialogFormVisible = true;\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    addCalendar() {\n      addSchedule(this.calendarForm).then(res => {\n        this.$message.success(\"添加成功\");\n      }).catch(error => {\n        console.log(error);\n      });\n      this.dialogFormVisible = false;\n    },\n    getElderList() {\n      axios.get(\"http://xiao.free.idcfengye.com/api/oldperson\").then(res => {\n        if (res.data.code === 0) {\n          this.elderList = res.data.data;\n        } else {\n          this.$message.error('获取老人信息失败');\n        }\n      }).catch(error => {\n        console.error(error);\n        this.$message.error('获取老人信息失败，请稍后再试');\n      });\n    },\n    delElder(item) {\n      axios.delete(`http://xiao.free.idcfengye.com/api/oldperson/${item.id}`).then(res => {\n        this.$message(\"删除成功\");\n        this.getElderList();\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    confirmDelete(item) {\n      MessageBox.confirm('确定要删除吗？', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        this.delElder(item);\n      }).catch(() => {});\n    },\n    add() {\n      this.oldMesTitle = '新增老人信息';\n      this.oldMesType = 3;\n      this.oldMesVisible = true;\n    },\n    viewMan(item) {\n      this.oldMesTitle = '查看老人信息';\n      this.oldMesType = 1;\n      axios.get(`http://xiao.free.idcfengye.com/api/oldperson/search`, {\n        params: {\n          id: item.id\n        }\n      }).then(res => {\n        this.form = res.data.data[0];\n        this.oldMesVisible = true;\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    updateMan(item) {\n      this.oldMesTitle = '修改老人信息';\n      this.oldMesType = 2;\n      axios.get(`http://xiao.free.idcfengye.com/api/oldperson/search`, {\n        params: {\n          id: item.id\n        }\n      }).then(res => {\n        this.form = res.data.data[0];\n        this.oldMesVisible = true;\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    updateOld() {\n      axios.put(`http://xiao.free.idcfengye.com/api/oldperson/${this.form.id}`, this.form).then(res => {\n        this.getElderList();\n      }).catch(error => {\n        console.log(error);\n      });\n      this.oldMesVisible = false;\n    },\n    addOld() {\n      axios.post(\"http://xiao.free.idcfengye.com/api/oldperson\", this.form).then(res => {\n        this.getElderList();\n      }).catch(error => {\n        console.log(error);\n      });\n      this.oldMesVisible = false;\n    },\n    handleSizeChange(value) {\n      this.listForm.size = value;\n      this.getElderList();\n    },\n    handlePageChange(value) {\n      this.listForm.page = value;\n      this.getElderList();\n    }\n  }\n};","map":{"version":3,"names":["axios","MessageBox","addOldSchedule","addSchedule","data","dialogVisible","listForm","page","size","total","pagerCount","genderOptions","value","label","elderList","gender","oldMesVisible","oldMesTitle","selectedOld","oldMesType","dialogFormVisible","form","dialogImageUrl","fileParam","id","username","phone","id_card","birthday","address","checkin_date","firstguardian_relationship","secondguardian_relationship","room_number","firstguardian_name","firstguardian_phone","firstguardian_wechat","secondguardian_name","secondguardian_phone","secondguardian_wechat","health_state","des","calendarForm","date","type","title","mounted","getElderList","methods","captureFace","row","navigator","mediaDevices","getUserMedia","video","then","stream","videoElement","document","getElementById","srcObject","chunks","recorder","MediaRecorder","ondataavailable","event","push","start","setTimeout","stop","getVideoTracks","onstop","formData","FormData","append","Blob","console","log","file","post","response","catch","error","finally","handleClose","done","calculateAge","birthDate","Date","today","age","getFullYear","monthDifference","getMonth","getDate","old_face","item","localStorage","setItem","$router","get","params","res","JSON","stringify","addDate","addCalendar","$message","success","code","delElder","delete","confirmDelete","confirm","confirmButtonText","cancelButtonText","add","viewMan","updateMan","updateOld","put","addOld","handleSizeChange","handlePageChange"],"sources":["src/views/page/old/oldMan.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <el-table :data=\"elderList\" border style=\"width: 100%\">\r\n      <el-table-column prop=\"username\" label=\"姓名\" width=\"120\" align=\"center\"></el-table-column>\r\n      <el-table-column\r\n          label=\"照片\"\r\n          width=\"120\"\r\n          align=\"center\" >\r\n        <template slot-scope=\"scope\">\r\n          <img :src=\"scope.row.imgset_dir\" alt=\"照片\" style=\"width: 100px; height: 100px;\" @click=\"captureFace(scope.row)\">\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"性别\" width=\"120\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <span>{{ gender[scope.row.gender] }}</span>\r\n        </template>\r\n      </el-table-column>\r\n\r\n      <el-table-column label=\"年龄\" width=\"100\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <span>{{ calculateAge(scope.row.birthday) }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"checkin_date\" label=\"入院日期\" width=\"120\" align=\"center\"></el-table-column>\r\n      <el-table-column prop=\"room_number\" label=\"房间号\" width=\"120\" align=\"center\"></el-table-column>\r\n      <el-table-column prop=\"health_state\" label=\"健康状况\" width=\"120\" align=\"center\"></el-table-column>\r\n\r\n      <el-table-column label=\"操作\" width=\"275\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"text\" size=\"small\" @click=\"viewMan(scope.row)\">查看</el-button>\r\n          <el-button type=\"text\" size=\"small\" @click=\"updateMan(scope.row)\">修改</el-button>\r\n          <el-button type=\"text\" size=\"small\" @click=\"captureFace(scope.row)\">上传人脸采集</el-button>\r\n          <el-button type=\"text\" size=\"small\" @click=\"confirmDelete(scope.row)\">删除</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handlePageChange\"\r\n        :page-sizes=\"[10, 20, 50, 100, 500, 1000]\"\r\n        :current-page=\"listForm.page\"\r\n        :page-size=\"listForm.size\"\r\n        :pagerCount=\"listForm.pagerCount\"\r\n        :total=\"listForm.total\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        style=\"margin: auto\">\r\n    </el-pagination>\r\n    <el-button class=\"add-button\" type=\"primary\" icon=\"el-icon-plus\" @click=\"add()\">添加</el-button>\r\n    <el-dialog :title=\"oldMesTitle\" :visible.sync=\"oldMesVisible\" width=\"900px\">\r\n      <el-form ref=\"form\" :model=\"form\" label-width=\"140px\" class=\"dialog-form\">\r\n        <el-form-item label=\"姓名\">\r\n          <el-input v-model=\"form.username\" :disabled=\"oldMesType !== 3\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"性别\">\r\n          <el-select v-model=\"form.gender\">\r\n            <el-option v-for=\"item in genderOptions\" :key=\"item.value\" :label=\"item.label\" :value=\"item.value\" :disabled=\"oldMesType !== 3\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"电话\" prop=\"phone\">\r\n          <el-input v-model=\"form.phone\" :disabled=\"oldMesType !== 3\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"身份证号\" prop=\"ID\">\r\n          <el-input v-model=\"form.id_card\" :disabled=\"oldMesType !== 3\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"出生日期\">\r\n          <el-input v-model=\"form.birthday\" :disabled=\"oldMesType !== 3\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"入院日期\">\r\n          <el-input v-model=\"form.checkin_date\" :disabled=\"oldMesType !== 3\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"房间号\">\r\n          <el-input v-model=\"form.room_number\" :disabled=\"oldMesType === 1\"></el-input>\r\n        </el-form-item>\r\n\r\n        <el-form-item label=\"第一监护人姓名\">\r\n          <el-input v-model=\"form.firstguardian_name\" :disabled=\"oldMesType === 1\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"与第一监护人关系\">\r\n          <el-input v-model=\"form.firstguardian_relationship\" :disabled=\"oldMesType === 1\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"第一监护人电话\">\r\n          <el-input v-model=\"form.firstguardian_phone\" :disabled=\"oldMesType === 1\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"第一监护人微信\">\r\n          <el-input v-model=\"form.firstguardian_wechat\" :disabled=\"oldMesType === 1\"></el-input>\r\n        </el-form-item>\r\n\r\n        <el-form-item label=\"第二监护人姓名\">\r\n          <el-input v-model=\"form.secondguardian_name\" :disabled=\"oldMesType === 1\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"与第二监护人关系\">\r\n          <el-input v-model=\"form.secondguardian_relationship\" :disabled=\"oldMesType === 1\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"第二监护人电话\">\r\n          <el-input v-model=\"form.secondguardian_phone\" :disabled=\"oldMesType === 1\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"第二监护人微信\">\r\n          <el-input v-model=\"form.secondguardian_wechat\" :disabled=\"oldMesType === 1\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"健康状态\">\r\n          <el-input v-model=\"form.health_state\" :disabled=\"oldMesType === 1\"></el-input>\r\n        </el-form-item>\r\n\r\n        <el-form-item style=\"width:100%;\" v-if=\"oldMesType===2\">\r\n          <el-button type=\"primary\" @click=\"updateOld()\" style=\"width:50%;margin-left:80px;margin-top:10px;border-color:#f3aecd;background-color: #f3aecd\">更改老人信息</el-button>\r\n        </el-form-item>\r\n        <el-form-item style=\"width:100%;\" v-if=\"oldMesType===3\">\r\n          <el-button type=\"primary\" @click=\"addOld()\" style=\"width:50%;margin-left:80px;margin-top:10px;border-color:#f3aecd;background-color: #f3aecd\">新增老人信息</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-dialog>\r\n\r\n    <el-dialog :visible.sync=\"dialogVisible\" width=\"36%\" height=\"36%\" :modal=\"false\" :show-close=\"false\" :before-close=\"handleClose\">\r\n    <div class=\"face-recognition-container\">\r\n        <video id=\"videoElement\" autoplay style=\"width: 100%;\"></video>\r\n        <div class=\"overlay\">\r\n          <p class=\"instructions\">信息采集中...</p>\r\n          <div class=\"scan-animation\"></div>\r\n        </div>\r\n      </div>\r\n    </el-dialog>\r\n\r\n\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport { MessageBox } from 'element-ui';\r\nimport { addOldSchedule, addSchedule } from \"@/api/schedule\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      dialogVisible: false, // 控制对话框显示的变量\r\n      listForm: {\r\n        page: 1,\r\n        size: 20,\r\n        total: 0,\r\n        pagerCount: 5,\r\n      },\r\n      genderOptions: [\r\n        { value: '0', label: '女' },\r\n        { value: '1', label: '男' }\r\n      ],\r\n      elderList: [],\r\n      gender: ['女', '男'],\r\n      oldMesVisible: false,\r\n      oldMesTitle: '查看老人信息',\r\n      selectedOld: 0,\r\n      oldMesType: 1, // 1仅查看2修改3新增\r\n      dialogFormVisible: false,\r\n      form: {\r\n        dialogImageUrl: \"\",\r\n        dialogVisible: false,\r\n        fileParam: \"\",\r\n        id: '',\r\n        username: '',\r\n        gender: '',\r\n        phone: '',\r\n        id_card: '',\r\n        birthday: '',\r\n        address: '',\r\n        checkin_date:'',\r\n        firstguardian_relationship:'',\r\n        secondguardian_relationship:'',\r\n        room_number:'',\r\n        firstguardian_name: '',\r\n        firstguardian_phone: '',\r\n        firstguardian_wechat: '',\r\n        secondguardian_name: '',\r\n        secondguardian_phone: '',\r\n        secondguardian_wechat: '',\r\n        health_state: '',\r\n        des: '',\r\n\r\n      },\r\n      calendarForm: {\r\n        date: '',\r\n        type: '',\r\n        title: '',\r\n      },\r\n    }\r\n  },\r\n  mounted() {\r\n    this.getElderList()\r\n  },\r\n  methods: {\r\n    captureFace(row) {\r\n      this.dialogVisible = true;\r\n\r\n      // Request access to user's camera\r\n      navigator.mediaDevices.getUserMedia({ video: true })\r\n          .then(stream => {\r\n            const videoElement = document.getElementById('videoElement');\r\n            videoElement.srcObject = stream;\r\n\r\n            const chunks = [];\r\n            const recorder = new MediaRecorder(stream);\r\n\r\n            recorder.ondataavailable = event => {\r\n              chunks.push(event.data);\r\n            };\r\n\r\n            recorder.start(5000); // Record for 5 seconds\r\n\r\n            setTimeout(() => {\r\n              recorder.stop();\r\n              stream.getVideoTracks()[0].stop(); // Stop camera stream\r\n            }, 5000);\r\n\r\n            recorder.onstop = () => {\r\n              // Create FormData and append recorded video data\r\n              const formData = new FormData();\r\n              formData.append('file', new Blob(chunks, { type: 'video/mp4' }));\r\n              formData.appe